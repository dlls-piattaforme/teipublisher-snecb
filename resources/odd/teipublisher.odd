                <outputRendition xml:space="preserve">
                
                
                
                
                
                
                
                
                
                
                
                
                
                
                -webkit-border-radius: 2px;
-moz-border-radius: 2px;
border-radius: 2px;
padding-left: 5px;
margin-top:10px;
background: linear-gradient(90deg, rgba(2,0,36,0.22170875186011907) 0%, rgb(255, 255, 255) 100%);
                
                
                
                
                
                
                
                
                
                
                
                
                
                
                </outputRendition>
            </model>
            <model predicate="@type='tag'" behaviour="link">
                <param name="content" value="switch(node()) case &#34;article in miscellaneous work&#34; return &#34;&#34; case &#34;article in proceedings&#34; return  &#34;&#34; case &#34;article in reference work&#34; return  &#34;&#34;                 case &#34;bibliography&#34;  return &#34;&#34; case &#34;digital projects &amp;amp; collections&#34; return &#34;&#34;               case &#34;edition&#34;   return &#34;&#34;                case &#34;edition/translation&#34; return  &#34;&#34;                 case &#34;facsimile&#34; return &#34;&#34;              case &#34;journal article&#34; return &#34;&#34;              case &#34;manuscript studies&#34; return &#34;&#34;                  case &#34;miscellaneous work&#34; return  &#34;&#34;                case &#34;monograph&#34;  return &#34;&#34;               case &#34;proceedings&#34; return  &#34;&#34;                 case &#34;reference work&#34; return &#34;&#34;                case &#34;review&#34;  return &#34;&#34;                 case &#34;thesis/dissertation&#34; return &#34;&#34;                  case &#34;translation&#34;  return  &#34;&#34;               default return node()"/>
                <param name="uri" value="concat ( '../snecb/home.html?facet-tag=', node())"/>
                <outputRendition xml:space="preserve">
                
                
                margin-right:5px;
padding:2px;
padding-left:3px;
